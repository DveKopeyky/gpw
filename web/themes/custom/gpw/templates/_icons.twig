{% macro get(icon, class, theme_path, width, height) %}
  <svg class="{{ class|default('') }}" width="{{ width|default(32) }}" height={{ height|default(32) }} aria-hidden="true">
    <use xlink:href={{ theme_path ~ '/dist/images/symbol/svg/sprite.symbol.svg#' ~  icon }} ></use>
  </svg>
{% endmacro %}
