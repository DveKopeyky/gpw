{% set id = entity['#child_entity'].title.value %}
<div {{ attributes }}>
    <div class="panel-group">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse-{{ id|clean_id }}">
                        {{ entity['title'] }} {{ entity['field_lectures_number'] }} {{ entity['field_videos_duration'] }}
                    </a>
                </h4>
            </div>
            <div id="collapse-{{ id|clean_id }}" class="panel-collapse collapse">
                <div class="panel-body">{{ entity.field_videos }}</div>
            </div>
        </div>
    </div>
</div>
