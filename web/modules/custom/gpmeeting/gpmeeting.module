<?php
/**
 * @file
 * Contains gpmeeting.module.
 */

use Drupal\node\NodeInterface;

/**
 * Implements hook_ENTITY_TYPE_presave().
 */
function gpmeeting_node_presave(NodeInterface $node) {
  if ($node->getType() == 'meeting' && !empty($node->field_date_range->value)) {
    $node->set('field_start_date', date('Y-m-d', strtotime($node->field_date_range->value)));
  }
}

